.space{
    margin-top: 94rpx;
    padding-bottom:200rpx;
    background:#FFF;
}
.choose-store-detail{
    display: flex;
    /*width: 100%;*/
    background-color: #ffffff;
    padding: 40rpx 0rpx;
    border-bottom: 1px solid #d9d9d9; 
}
.choose-store-detail .one{
    display: flex;
    align-items: center;
    justify-content: center;
    width: 20%
}
.choose-store-detail .two{
    width: 60%;
}
.choose-store-detail .two text{
    display: block;
    overflow: hidden;
    text-overflow:ellipsis;
    white-space: nowrap;
    margin-bottom: 10rpx;
}
.choose-store-detail .store-item .star{
    display: block;
    width: 124rpx;
    height: 24rpx;
    margin-bottom: 10rpx;
}
.choose-store-detail .three{
    text-align: center;
    width: 20%;
}
.has-choose {
    display: flex;
    flex-direction: column;
    padding: 40rpx;
}
.has-choose text{
    display: block;
    font-weight:bold; 
    overflow: hidden;
    text-overflow:ellipsis;
    white-space: nowrap; 
    line-height: 50rpx;  
}
.has-choose-footer{
    position: fixed;
    bottom: 0;
    width: 100%;
    border-top:1px solid #d9d9d9;
    background-color: #ffffff;
}
.has-choose-footer button{
    background-color: #FF8F00;
    color: #ffffff;
    border-radius:0; 
}
