.page{
    background-color: #FFFFFF;
}
.title {
    color:#FFFFFF;
    position:absolute;
    z-index:10;
    top:15px;
    left:50%;
    text-align:center;
    transform:translateX(-50%);
}
.image {
    width:750rpx;
    height:910rpx;
    position: relative;
    overflow: hidden;
    background-color: #FF9801;
}
.moofunIndex {
    position:absolute;
    width:100%;
    height:70%;
    top:0;
    left:0;
}
.barcode {
    position:absolute;
    bottom: 100rpx;
    left:50%;
    width:242rpx;
    height:242rpx;
    transform:translateX(-50%);
}
.tips {
    color:#FFFFFF;
    position:absolute;
    z-index:10;
    bottom: 40rpx;
    left:50%;
    text-align:center;
    font-size: 28rpx;
    white-space:nowrap;
    transform:translateX(-50%);
}
.bottom{
    height: 19%;
    background-color: #FFFFFF;
}
.bottom image{
    width: 100%;
    height: 100%;
}
.ps {
    font-size:24rpx;
    display:block;
    text-align:center;
    color: #9B9B9B;
    line-height: 36px;
}