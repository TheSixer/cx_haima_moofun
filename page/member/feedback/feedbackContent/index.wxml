<view class="page">
  <view class="body">
    <view class="section">
      <view>
          <textarea bindinput="input" placeholder="请输入您的反馈内容" />
      </view>
      <view>
          <button class="submit" disabled="{{btnDis}}" bindtap="submit"> 提交反馈 </button>
      </view> 
    </view>
    <view class="footer" bindtap="urlTo">
          <navigator class="navigator" hover-class="widget-hover" url="../feedbackRecord/index">
              <text>我的反馈记录</text>
          </navigator>
    </view>
  </view>
</view>
