.body{
    background-color: #ffffff;
    font-size: 14px;
}
.set,.set-detail-content{
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 30rpx 10rpx 30rpx;
    border-bottom: 1px solid #EBEBEB;
    margin-left: 20rpx;
    font-weight: bold;
}
.footer{
    text-align: center;
}
.footer text{
    color: #A1A1A1;
    font-size:12px; 
    line-height:100rpx;
}
.footer button{
    width:92%;
    color: #ffffff;
    background-color: #FF9801;
}
.set-detail-content text{
    display: inline-block;
}
.set-detail-content .time{
    display: flex;
    align-items: center;
    padding-right:40rpx;
}
.time input {
    text-align:right;
}
.set-detail-content .left{
    margin-right: 10rpx;
}
.set-detail-content image{
    width: 40rpx;
    height: 40rpx;
}
.set-detail .line{
    width: 100%;
    height: 20rpx;
    background-color: #F2F2F2;
}
.set-detail .last{
    border: none;
}
.set-detail .tips{
    display: flex;
    line-height:50rpx;
    background-color: #F2F2F2;
    font-size: 12px;
    color: #9B9B9B;
    padding: 30rpx 10rpx 30rpx;
}
.set-detail .tips-left{
    display: inline-block;
    margin-left:20rpx;
}
.set-detail .tips-right{
    display: block;
}