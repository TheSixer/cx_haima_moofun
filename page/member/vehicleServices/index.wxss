/* page/member/vehicleServices/index.wxss */
.body {
    height: 100%;
    width: 100%;
}
.page__bd {
    height: 100%;
}
.selectService {
    height: 50px;
    display: flex;
    padding: 16rpx 40rpx;
    box-sizing: border-box;
}
.selectService button {
    height: 32px;
    line-height: 32px;
    width: 33.3333%;
    color: #FF9801;
    font-size: 14px;
    border-radius: 0;
    border: 1px solid #FF9801;
}
.selectService button.selected {
    color: #fff;
    background: #FF9801;
}
.selectService button:first-child {
    border-right: none;
    border-top-left-radius: 6px;
    border-bottom-left-radius: 6px;
}
.selectService button:last-child {
    border-left: none;
    border-top-right-radius: 6px;
    border-bottom-right-radius: 6px;
}
.selectService button:after {
    content: none;
}
.markerInfo {
    position: fixed;
    left:0;
    bottom:0;
    width:100%;
    height:80px;
    z-index:1000;
    background:#fff;
}
.goBtn {
    display:inline-block;
    position:absolute;
    right:30rpx;
    top:15px;
}
.goBtn image {
    width: 100rpx;
    height: 100rpx;
}
.info {
    /*padding:40rpx 30rpx;*/
    padding-right: 75px; 
}
.title {
    padding: 10px 15px;
    font-weight: bold;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.address {
    padding: 0 15px;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    font-size: 14px;
}
