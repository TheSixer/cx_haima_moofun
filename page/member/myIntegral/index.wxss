/* page/member/myIntegral/index.wxss */
.body {
    /*height: 100%;*/
    width: 100%;
    background-color: #FFFFFF;
}
.flex-wrp{
    height: 200rpx;
    display:flex;
    background-color: #FFFFFF;
    flex-direction:row;
    justify-content:space-between;
    align-items: center;
    margin-left: 32rpx;
    margin-right: 32rpx;
}
.flex-item{
    display: flex;
    flex-direction:column; 
}
.flex-item-inside{
    margin-bottom: 20rpx;
    font-weight: bold;
}
.flex-item-inside .mark {
    display: inline-block;
    color: #F6AA2C;
    font-size: 30px;
}
.flex-item-line{
    height: 20rpx;
    background-color: #F2F2F2;
}
.explain{
    font-size: 12px;
    color: #AAAAAA;
}
.flex-wrp-list{
    height: 100rpx;
    padding: 15px 0;
    border-bottom: 1px solid #F4F4F4;
}
.flex-wrp-list .flex-item-inside{
    margin-bottom: 10rpx;
}
.flex-item .content-explain{
    color: #AAAAAA;  
    font-size: 14px;
    font-weight: normal;
}
.flex-wrp .number{
    display: inline-block;
    width: 160rpx;
    height: 56rpx;
    color: #FFFFFF;
    text-align: center;
    line-height: 60rpx;
    border-radius:20px;
    background-color:#F7A700; 
}